    <script id="rendered-js">
        var navItems = document.querySelectorAll(".mobile-bottom-nav__item");
        navItems.forEach(function (e, i) {
            e.addEventListener("click", function (e) {
                navItems.forEach(function (e2, i2) {
                    e2.classList.remove("mobile-bottom-nav__item--active");
                });
                this.classList.add("mobile-bottom-nav__item--active");
                if (this.id == "income") {
                    window.location.href = "http://www.bioappsystem.com/bioappmalay/IncomeStatetement2Mobile.aspx?userid=<%=sUserId%>";
                }
                if (this.id == "finance") {
                    window.location.href = "http://www.bioappsystem.com/bioappmalay/CashFlowMobile.aspx?userid=<%=sUserId%>";
                }
                if (this.id == "stock") {
                    window.location.href = "http://www.bioappsystem.com/bioappmalay/StockStateMobile.aspx?userid=<%=sUserId%>";
                }
                if (this.id == "zakat") {
                    window.location.href = "http://www.bioappsystem.com/bioappmalay/ZakatCalculationMobile.aspx?userid=<%=sUserId%>";
                }
            });
        });

        function getParameterByName(name) {
            name = name.replace(/[[]/, "\[").replace(/[]]/, "\]");
            var regex = new RegExp("[\?&]" + name + "=([^&#]*)"), results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }

    </script>

