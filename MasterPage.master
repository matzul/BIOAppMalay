    <script id="view-sticky-icon">
        function PageMethod2(fn, paramArray, successFn, errorFn, asyncFn) {
            var pagePath = 'http://www.bioappsystem.com/bioappmalay/WebService.asmx';
            var paramList = '';

            if (paramArray.length > 0) {
                for (var i = 0; i < paramArray.length; i += 2) {
                    if (paramList.length > 0) paramList += ',';
                    paramList += '"' + paramArray[i] + '":"' + paramArray[i + 1] + '"';
                }
            }
            paramList = '{' + paramList + '}';
            //Call the page method
            $.ajax({
                type: "POST",
                url: pagePath + "/" + fn,
                contentType: "application/json; charset=utf-8",
                data: paramList,
                dataType: "json",
                success: successFn,
                timeout: 600000,
                error: errorFn,
                async: asyncFn
            });
        }

        function viewInfo(sType) {
            var hidden = $('.message-window');
            var sizeScreen = window.matchMedia("(max-width: 850px)");

            $('#records_table').empty();
            $('#records2_table').empty();

            if (hidden.hasClass('visible')) {

                hidden.animate({ "left": "100%" }, "slow", function () { $('#container-slide').hide(); }).removeClass('visible');
                $('.container-slide').css('overflow', "hidden");

            } else {

                //set iframe
                //setTimeout(function () { $('#infoFoodbank').attr('src', 'http://www.bioappsystem.com/bioappmalay/IncomeStatetement2Mobile.aspx?userid=' + userid); console.log("set iframe source") }, 500);

                if (sType == 'PAYMENT') {
                    $("#sTitle").text("Senarai Invois & Bil Semasa");
                    setTimeout(function () {

                        var parameters_getMobile_PendingPayRcptList = ["comp", "<%=sCurrComp%>", "bpid", "", "invoiceno", ""];
                        PageMethod2("getMobile_PendingPayRcptList", parameters_getMobile_PendingPayRcptList, succeededAjaxFn_getMobile_PendingPayRcptList, failedAjaxFn_getMobile_PendingPayRcptList, false);

                        var parameters_getMobile_PendingPayPaidList = ["comp", "<%=sCurrComp%>", "bpid", "", "expensesno", ""];
                        PageMethod2("getMobile_PendingPayPaidList", parameters_getMobile_PendingPayPaidList, succeededAjaxFn_getMobile_PendingPayPaidList, failedAjaxFn_getMobile_PendingPayPaidList, false);

                    }, 500);
                }

                if (sType == 'STOCK') {
                    $("#sTitle").text("Senarai Stok & Inventori Semasa");
                    setTimeout(function () {

                        var parameters_getMobile_StockList = ["comp", "<%=sCurrComp%>", "searchitem", ""];
                        PageMethod2("getMobile_StockList", parameters_getMobile_StockList, succeededAjaxFn_getMobile_StockList, failedAjaxFn_getMobile_StockList, false);

                    }, 500);
                }

                if (sType == 'CASH') {
                    $("#sTitle").text("Senarai Tunai Tangan & Bank");
                    setTimeout(function () {

                        var parameters_getMobile_CashFlowHeaderList = ["comp", "<%=sCurrComp%>", "cashflowno", "", "openingdate", "", "openingtype", "", "closingdate", "", "closingtype", "", "status", "IN-PROGRESS"];
                        PageMethod2("getMobile_CashFlowHeaderList", parameters_getMobile_CashFlowHeaderList, succeededAjaxFn_getMobile_CashFlowHeaderList, failedAjaxFn_getMobile_CashFlowHeaderList, false);

                    }, 500);
                }

                $('#container-slide').show();
                if (sizeScreen.matches) {
                    $('.message-window').css('width', "100%");
                    hidden.animate({ "left": "0" }, "slow").addClass('visible');
                } else {
                    $('.message-window').css('width', "45%");
                    hidden.animate({ "left": "55%" }, "slow").addClass('visible');
                }

                $('.container-slide').css('overflow', "");
            }
        }

