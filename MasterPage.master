        function openpossystem(comp, userid) {
            var popupWindow = window.open("POS/index.html?currcomp=" + comp+"&userid=" + userid, "open_pos", "toolbar=0,location=0,status=1,menubar=0,resizable=1,scrollbars=1,width=1020,height=800");
            if (popupWindow == null) {
                alert("Error: While Launching Session Expiry screen.\nYour browser maybe blocking up Popup windows.\nPlease check your Popup Blocker Settings");
            } else {
                wleft = (screen.width - 1020) / 2;
                wtop = (screen.height - 800) / 2;
                if (wleft < 0) {
                    wleft = 0;
                }
                if (wtop < 0) {
                    wtop = 0;
                }
                popupWindow.moveTo(wleft, wtop);
            }
